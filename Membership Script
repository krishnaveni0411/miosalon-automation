	 Select selectTheOption = new Select(selectPriceType);
	 selectTheOption.selectByVisibleText("Value");
	 WebElement provideTheDiscountvalue = driver.findElement(By.xpath("(//div[@class='input-group']/input[@type='number'])[2]"));
	 provideTheDiscountvalue.click();
	 provideTheDiscountvalue.clear();
	 generateRandomPercentageValue();
