 public static void updateMembershipInBranch2() throws InterruptedException{
	 Thread.sleep(2000);
	 List<WebElement> listOfMembership = driver.findElements(By.xpath("//table[@class='table ng-scope']/tbody/tr"));
	 listOfMembership.get(1).click();
	 driver.findElement(By.xpath("//button[text()='Update']")).click();
	 String getTheSuccessMessage = driver.findElement(By.xpath("//span[@data-notify='title']")).getText();
	 driver.findElement(By.xpath("//button[@class='close']")).click();
	System.out.println("Get The Success Message : " + getTheSuccessMessage);
	 System.out.println("the seleccted membership is updated"); 
 }
 public static void deleteMembershipInBranch2() throws InterruptedException{
	 Thread.sleep(2000);
	 List<WebElement> listOfMembership = driver.findElements(By.xpath("//table[@class='table ng-scope']/tbody/tr"));
	 listOfMembership.get(1).click();
	 driver.findElement(By.xpath("//button[text()='Delete']")).click();
	 driver.findElement(By.xpath("//input[@id='confirmCkeck']")).click();
	 driver.findElement(By.xpath("(//button[text()='Delete'])[last()]")).click();
	 String getTheSuccessMessage = driver.findElement(By.xpath("//span[@data-notify='title']")).getText();
	 driver.findElement(By.xpath("//button[@class='close']")).click();
	System.out.println("Get The Success Message : " + getTheSuccessMessage);
	 System.out.println("the seleccted membership is deleted"); 
 }
 public static void errorMessageWhileCreatingInBranch2() throws InterruptedException{
	 Thread.sleep(2000);
	 driver.findElement(By.xpath("//button[text()='Add Discount Membership']")).click();
	 driver.findElement(By.xpath("//button[text()='Save']")).click();
	 String errorInTheName = driver.findElement(By.xpath("//small[@class='error ng-scope']")).getText();
     System.out.println("error In The Name Of The Membership : " + errorInTheName);
     String errorInThevalue = driver.findElement(By.xpath("(//small[@class='error ng-scope'])[2]")).getText();
     System.out.println("error In The price Of The Membership : " + errorInThevalue);
     String errorInThevalidity = driver.findElement(By.xpath("(//small[@class='error ng-scope'])[last()]")).getText();
     System.out.println("error In The validity Of The Membership : " + errorInThevalidity);
     driver.findElement(By.xpath("//button[text()='Cancel']")).click();
     
 }

