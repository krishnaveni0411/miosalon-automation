 public static void addServiceAndProductWithPercentage() throws InterruptedException{
	 Thread.sleep(2000);
	 driver.findElement(By.xpath("//button[text()='Add Discount Membership']")).click();
	 WebElement provideName = driver.findElement(By.xpath("//input[@data-ng-model='membership.name']"));
	 provideName.click();
	 generateRandomName(6);
	 String randomNameOfTheMS= generateRandomName(6);
	 provideName.sendKeys(randomNameOfTheMS);
	 WebElement ProvidePrice = driver.findElement(By.xpath("//input[@data-ng-model='membership.price']"));
	 ProvidePrice.click();
	 generateRandomPrice();
	 int randomPriceOfTheMS= generateRandomPrice();
	 ProvidePrice.sendKeys(String.valueOf(randomPriceOfTheMS));
	 WebElement tax = driver.findElement(By.xpath("//select[@name='tax']"));
	 Select selectTheTax =new Select(tax);
	 selectTheTax.selectByVisibleText("GST(18)");
	 WebElement provideValidity = driver.findElement(By.xpath("//input[@data-ng-model='membership.validity_days']"));
	 provideValidity.click();
	 generateRandomValidity();
	 int randomValidity = generateRandomValidity();
	 provideValidity.sendKeys(String.valueOf(randomValidity));
	 WebElement provideTheDiscountPercent = driver.findElement(By.xpath("(//div[@class='input-group']/input[@type='number'])[2]"));
	 provideTheDiscountPercent.click();
	 provideTheDiscountPercent.clear();
	 generateRandomPercentageValue();
	 int randomDiscountPercentage = generateRandomPercentageValue();
	 provideTheDiscountPercent.sendKeys(String.valueOf(randomDiscountPercentage));
	 driver.findElement(By.xpath("//button[text()='Apply All']")).click();
	// select the item as product
		// driver.findElement(By.xpath("//button[@data-id='itemtype']")).click();
		 WebElement selectTheItemType = driver.findElement(By.xpath("//select[@id='itemtype']"));
		 Select chooseTheItem= new Select(selectTheItemType);
		 chooseTheItem.selectByVisibleText("Product");
		 WebElement provideTheDiscountPercentForProduct = driver.findElement(By.xpath("(//div[@class='input-group']/input[@type='number'])[2]"));
		 provideTheDiscountPercentForProduct.click();
		 provideTheDiscountPercentForProduct.clear();
		 generateRandomPercentageValue();
		 int randomDiscountPercentageForProduct = generateRandomPercentageValue();
		 provideTheDiscountPercentForProduct.sendKeys(String.valueOf(randomDiscountPercentageForProduct));
		 driver.findElement(By.xpath("//button[text()='Apply All']")).click();
	 driver.findElement(By.xpath("//button[text()='Save']")).click();
	 String getTheSuccessMessage = driver.findElement(By.xpath("//span[@data-notify='title']")).getText();
	 driver.findElement(By.xpath("//button[@class='close']")).click();
	System.out.println("Get The Success Message : " + getTheSuccessMessage);
	
 }
